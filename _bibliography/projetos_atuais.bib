@article{lemes2023,
abstract={A doula é uma profissional que, a partir de evidências científicas, atua diretamente no acompanhamento de mulheres durante todo o ciclo gravídico-puerperal, oferecendo informações e prestando o apoio que se faz necessário durante todo esse processo. Estudos comprovam que o apoio desta profissional reduz significativamente os índices de intervenções no momento do parto, bem como, auxilia na diminuição de incidência de depressão pós-parto e promove uma melhora nos índices relacionados à amamentação. Atualmente, a ocupação de doula passou a constar no Cadastro Brasileiro de Ocupações, sendo reconhecida então, pelo Ministério do Trabalho, cujos direitos trabalhistas passaram a ser assegurados. Diante dessas informações e levando em consideração a missão do IFG, a proposta da extensão é oportunizar a qualificação de mulheres residentes em municípios com alto índice de vulnerabilidade, que sejam maiores de 18 anos e possuam o ensino fundamental incompleto. Assim, o programa de extensão visa ofertar um curso de formação e eventos de extensão articulando o ensino, a pesquisa e a extensão. O curso de formação será realizado através do ensino híbrido e contará com a parceria do Instituto Matriusca e com a Associação de Doulas do Distrito Federal e entorno. Serão abordados temas que vão desde leitura e produção de texto, ética, informática, empreendedorismo, até as disciplinas voltadas para a área específica. A avaliação do processo de aprendizagem será realizada de forma contínua e sistemática ao longo de todo o curso e ao final do mesmo, espera-se a oportunização da inclusão dessas mulheres no contexto social e econômico.},
abbr={Extensão},
author={Mario Lemes and outros},
journal={Instituto Matriusca & Instituto Federal de Goiás - Campus Águas Lindas, Luziânia, Formosa e Valparaíso}, 
title={Curso de Doula - EDITAL 04/2023/PROEX/IFG}, 
year={2023-2},
}


@article{lemes2023-pre,
abstract={Este presente projeto de extensão tem a finalidade de corroborar afirmações de ações interdisciplinares socio empreendedoras de sensibilização, prospecção e pré-incubação regional do núcleo incubador do Câmpus Formosa. Neste sentido, estudantes e comunidade externa desenvolverão projetos interdisciplinares de pesquisa, inovação, sensibilização, prospecção de empreendimentos de base social e tecnológica da comunidade regional ao qual abrande o Câmpus Formosa, sob a participação, acompanhamento e orientação de toda a equipe que compõe o núcleo incubador do Câmpus. A proposta é reservar uma carga horária adequada aos estudantes e comunidade externa, para que estes explorem sua curiosidade e criatividade, trazendo seus interesses e "dores" ao contexto escolar e de pesquisa, visando a agregação de valores e ao desenvolvimento regional, podendo aprofundar conhecimentos a partir de temas relevantes a estes e a sociedade; relacionados a inovação, empreendedorismo de base tecnológica e social. Prospecta-se a articulação entre pesquisa, ensino e extensão; possibilitando novos caminhos aos estudantes e a comunidade externa regional, cujo objetivo principal será culminar na pré-incubação de ideias inovadoras e empreendimentos durante todo o processo de ações, formações, eventos, socialização e articulação de ideias e empreendimentos novos e já consolidados na região..},
abbr={Extensão},
author={Mario Lemes and outros},
journal={Instituto Federal de Goiás - Campus Formosa}, 
title={Ações interdisciplinares socio empreendedoras de sensibilização, prospecção e pré-incubação regional do núcleo incubador do Câmpus Formosa - EDITAL 04/2023/PROEX/IFG}, 
year={2023-2},
}




