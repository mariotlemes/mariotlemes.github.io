@article{luizfelipe,
 abstract={Esse projeto de pesquisa propõe o desenvolvimento de uma página web que integre um modelo de processamento de linguagem natural ao sistema de monitoramento Zabbix, tornando as métricas e alertas técnicos de infraestrutura mais acessíveis a usuários não especializados. Ao coletar dados em tempo real sobre disponibilidade, desempenho e segurança de redes, servidores e aplicações acadêmicas, o sistema utilizará técnicas de sumarização para geração de relatórios simplificados, livres de jargões técnicos. Esses relatórios serão exibidos em uma interface web, isolada do front-end do Zabbix, com recursos adicionais de acessibilidade, de forma que gestores e pessoas com deficiência visual ou motora compreendam e interajam eficientemente com as informações geradas. O desenvolvimento deste projeto contribuirá com a tomada de decisão institucional e assegurará a continuidade dos serviços relacionados ao ensino, pesquisa e extensão do IFG com maior simplificação, democratização das informações e economia de recursos.},
 author={Luiz Felipe de Castro Santana},
 title={Interface web simplificada e com acessibilidade para compreensão dos dados monitorados pelo Zabbix},
 year = {2025},
 journal = {Programa Institucional de Bolsas de Iniciação em Desenvolvimento Tecnológico e Inovação - Edital nº 22},
 abbr={PIBIT},
 preview={luiz.png},
}

@article{caue,
 abstract={As ferramentas digitais como realidade virtual, realidade aumentada, aplicativos para dispositivos móveis e efeitos visuais interativos têm revolucionado a forma como o público vivencia apresentações musicais ao vivo. Eventos culturais incorporam iluminação, projeções e ambientes envolventes que intensificam a conexão sensorial com os shows. Embora estas inovações estejam se tornando mais comuns no setor do entretenimento, ainda são pouco exploradas na literatura acadêmica. Esse projeto de pesquisa propõe realizar uma revisão sistemática da literatura sobre a aplicação de tecnologias digitais na mediação da experiência musical ao vivo. A metodologia exploratória e de mapeamento permitirá avaliar a importância científica do tema, entender as metodologias empregadas e identificar quais aspectos são mais frequentemente abordados. Adicionalmente, a pesquisa pretende identificar lacunas e tendências, fornecendo informações valiosas para investigações futuras. Ao final, espera-se apresentar uma visão abrangente do estado da arte das interações entre tecnologia e música ao vivo, contribuindo para o avanço em performances artísticas e estudos culturais.},
 author={Caue Rafael Evangelista de Jesus},
 title={Investigação da experiência musical ao vivo mediada por tecnologias digitais: uma revisão sistemática da literatura},
 year = {2025},
 journal = {Programa Institucional de Bolsas de Iniciação Científica - Edital nº 21},
 abbr= {PIBIC},
 preview={caue.png},
}


@article{giulia_leticia,
 abstract={Este TCC aborda a concepção e construção de um sistema de conversação (chatbot) contextualizado para elucidar dúvidas específicas da comunidade acadêmica do Instituto Federal de Goiás Campus Formosa. A integração do sistema de perguntas e repostas a grandes modelos de linguagem, aliado ao emprego de técnicas de indexação de documentos específicos e inerentes ao ensino, pesquisa e extensão, proporcionam respostas eficientes não somente às questões da comunidade interna, professores/as, técnicos/as administrativos/as, alunos/as, mas também da comunidade externa, a respeito de diferentes atividades executadas pelo Instituto. A pesquisa enfatiza o emprego da tecnologia de Inteligência Artificial como fator chave no suporte acadêmico e divulgação de informações presentes em documentos oficiais produzidos por diferentes setores da Instituição.},
author={Letícia Bittencourt Vieira | Giulia Caroliny Vera Martins},
title= {TremBot: sistema de conversação automatizado e especializado para o IFG campus Formosa},
year = {2025},
journal={TCC do Curso Superior de Tecnologia em Análise e Desenvolvimento de Sistemas},
abbr = {TCC},
preview={leticia_giulia2.png},
}

@article{eloisa,
author= {Eloisa Graziele Rodrigues de Araujo},
abstract = {A coleta manual de presença é uma prática comum em muitas escolas municipais, contudo,
essa metodologia pode resultar em desperdício de tempo durante as aulas e propiciar erros
humanos. A evasão escolar é um problema persistente que afeta uma parcela significativa dos
alunos da rede pública. Diversos fatores contribuem para esse fenômeno, sendo um dessas a
falta de envolvimento dos responsáveis na vida escolar dos estudantes, muitas vezes devido a
questões relacionadas à estrutura familiar. A presença regular dos alunos nas atividades escolares
é fundamental para o bom desempenho acadêmico. No entanto, o processo de registro da
frequência nem sempre é eficiente. Com o intuito de solucionar essa problemática, esse trabalho
aborda o desenvolvimento um sistema automatizado de controle de presença, que visa otimizar
e agilizar o processo de registro de frequência dos alunos. O sistema utiliza um dispositivo
ESP8266 conectado a um leitor de QRCode, permitindo que a presença dos alunos seja registrada
de forma automática, sem necessidade de intervenção manual. Cada aluno recebe um crachá com
um QRCode exclusivo, que é lido pelo dispositivo, e as informações de presença são transmitidas
em tempo real para uma página web. Dessa forma, os pais podem acessar a plataforma para
acompanhar a frequência de seus filhos, promovendo acompanhamento mais eficaz da vida
escolar dos estudantes e incentivando maior envolvimento dos pais e responsáveis no processo
educacional.},
title={Sistema de controle de presença com
ESP8266 e QR code para escolas municipais},
year = {2025},
journal={TCC do Curso Superior de Tecnologia em Análise e Desenvolvimento de Sistemas},
abbr={TCC},
preview={eloisa2.jpg}
}

@article{ruan,
author= {Ruan Rocha de Souza Feitosa},
abstract = {A predição da geração de energia solar é essencial para garantir a estabilidade das redes elétricas, otimizar o planejamento energético e ampliar a integração de fontes renováveis de forma segura e eficiente. Este TCC apresenta um estudo sobre a utilização de algoritmos de aprendizado de máquina na predição de energia solar fotovoltaica. Foram analisados os algoritmos Árvore de Decisão (AD), Floresta Aleatória (FA) e Perceptron Multicamada (PM), bem como suas combinações em modelos híbridos duplos e em uma configuração híbrida tripla. A pesquisa explorou dados meteorológicos públicos de geração de energia solar de três locais distintos, avaliados por meio das métricas de Erro Médio Absoluto (EMA), Raiz do Erro Quadrático Médio (REQM) e Coeficiente de Determinação (R²). Os resultados indicam que os modelos híbridos superaram os individuais, com destaque para a modelagem híbrida composta pela Floresta Aleatória e Perceptron Multicamada (Modelo 6), que apresentou reduções de até 12,5\% no EMA, melhorias de aproximadamente 6,6\% no REQM e valores de R² superiores a 0,90 em todos os locais. Embora o modelo triplo tenha alcançado desempenho próximo em alguns cenários, não conseguiu superar o Modelo 6 de forma consistente. Os resultados revelam o potencial das abordagens híbridas para aumentar a confiabilidade na predição de potência gerada por sistemas fotovoltaicos, contribuindo positivamente para o planejamento e a operação de sistemas energéticos sustentáveis.},
title={Aplicação de estratégias de aprendizado de máquina ao problema de predição de energia solar fotovoltaica},
year = {2025},
journal={TCC do Curso Superior de Tecnologia em Análise e Desenvolvimento de Sistemas},
abbr={TCC},
preview={ruan2.jpg}
}

@article{jennifer_matheus,
author= {Jeniffer Santana Pereira Santos},
abstract = {A assistência em maternidades envolve lidar com eventos sensíveis e complexos que exigem
uma comunicação cuidadosa e segura. A divulgação dessas informações aos profissionais de
saúde ocorre muitas vezes de maneira inadequada, utilizando quadros físicos que expõem dados
sensíveis e falham em garantir a privacidade, resultando em uma assistência desumanizada.
Diante dessa problemática, este trabalho apresenta o desenvolvimento do sistema “Magnólia”,
uma solução web voltada para a gestão humanizada e segura de informações clínicas em maternidades. O objetivo principal foi criar um software capaz de categorizar riscos automaticamente e
apresentar dados vitais de forma simplificada, apoiando a tomada de decisão clínica e respeitando
os princípios da lei geral de proteção de dados. A metodologia de desenvolvimento seguiu uma
abordagem ágil e incremental, utilizando tecnologias consolidadas: o framework Laravel para o
back-end com arquitetura Model-View-Controller (MVC), a biblioteca React para o front-end no
modelo Single Page Application (SPA) e o banco de dados MySQL. Como resultados, Magnólia
integra controle de acesso robusto com autenticação de dois fatores, painéis administrativos para
gestão de métricas e um módulo clínico que utiliza indicadores visuais para classificação de risco.
A solução proposta preenche a lacuna tecnológica identificada, promovendo uma comunicação
empática e otimizando os processos de admissão, evolução e alta, garantindo assim a integridade
da informação e a humanização do cuidado às pacientes.},
title={Magnólia: sistema humanizado de divulgação de informações de saúde das pacientes em maternidade},
year = {2025},
journal={TCC do Curso Superior de Tecnologia em Análise e Desenvolvimento de Sistemas},
abbr={TCC},
preview={jennifer.jpg}
}