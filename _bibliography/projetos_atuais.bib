@article{lemes2023-pre,
abstract={Este presente projeto de extensão tem a finalidade de corroborar afirmações de ações interdisciplinares socio empreendedoras de sensibilização, prospecção e pré-incubação regional do núcleo incubador do Câmpus Formosa. Neste sentido, estudantes e comunidade externa desenvolverão projetos interdisciplinares de pesquisa, inovação, sensibilização, prospecção de empreendimentos de base social e tecnológica da comunidade regional ao qual abrande o Câmpus Formosa, sob a participação, acompanhamento e orientação de toda a equipe que compõe o núcleo incubador do Câmpus. A proposta é reservar uma carga horária adequada aos estudantes e comunidade externa, para que estes explorem sua curiosidade e criatividade, trazendo seus interesses e "dores" ao contexto escolar e de pesquisa, visando a agregação de valores e ao desenvolvimento regional, podendo aprofundar conhecimentos a partir de temas relevantes a estes e a sociedade; relacionados a inovação, empreendedorismo de base tecnológica e social. Prospecta-se a articulação entre pesquisa, ensino e extensão; possibilitando novos caminhos aos estudantes e a comunidade externa regional, cujo objetivo principal será culminar na pré-incubação de ideias inovadoras e empreendimentos durante todo o processo de ações, formações, eventos, socialização e articulação de ideias e empreendimentos novos e já consolidados na região..},
abbr={Extensão},
author={Mario Lemes and outros},
journal={Instituto Federal de Goiás - Campus Formosa}, 
title={Ações interdisciplinares socio empreendedoras de sensibilização, prospecção e pré-incubação regional do núcleo incubador do Câmpus Formosa - EDITAL 04/2023/PROEX/IFG}, 
year={2023-2},
}




